{
  "ai": {
    "enableAutoCompletion": true,
    "enableInlineCompletion": true,
    "enableChat": true
  },
  "editor": {
    "formatOnSave": true,
    "tabSize": 4,
    "insertSpaces": true
  },
  "search": {
    "excludePatterns": [
      "**/node_modules/**",
      "**/vendor/**",
      "**/build/**",
      "**/.git/**"
    ]
  }
} 